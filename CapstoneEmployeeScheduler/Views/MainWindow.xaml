<Window x:Class="CapstoneEmployeeScheduler.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CapstoneEmployeeScheduler"
        mc:Ignorable="d"
        Title="Capstone Employee Scheduler: Home" Height="750" Width="1000" MinHeight="750" MinWidth="1000">
    <Grid Background="#FFB9B9B9">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition/>
            <ColumnDefinition Width="Auto"/>
        </Grid.ColumnDefinitions>
        <Frame x:Name="Main" NavigationUIVisibility="Hidden" Grid.ColumnSpan="3" Navigated="Main_Navigated" Margin="0,0,-0.4,-0.2" Padding="0" Background="#FF84C8FF"/>
        <Button x:Name="Employees" Content="Employees" HorizontalAlignment="Left" VerticalAlignment="Top" Width="124" Margin="0,287,0,0" Height="74" Click="Employees_Click_1" Grid.Column="1" RenderTransformOrigin="-2.887,0.683" Background="#FFDDDDDD" BorderBrush="#FF888888" Foreground="Black" FontWeight="Bold"/>
        <!--<Button x:Name="TestingBackend" Content="TestingBackend" HorizontalAlignment="Left" VerticalAlignment="Top" Width="98" Margin="5,400,0,0" Height="29" Click="TestingBackend_Click" Grid.Column="1" FontWeight="Bold"/>-->
        <Button x:Name="Roles" Grid.ColumnSpan="2" Content="Roles" HorizontalAlignment="Left" VerticalAlignment="Top" Width="124" Margin="0,361,0,0" Height="74" Click="Button_Click_1" Background="#FFDDDDDD" BorderBrush="#FF888888" Foreground="Black" FontWeight="Bold"/>
        <Button x:Name="History" Grid.ColumnSpan="2" Content="History" HorizontalAlignment="Left" Margin="0,213,0,0" VerticalAlignment="Top" Width="124" Height="74" Click="History_Click_1" BorderBrush="#FF888888" Foreground="Black" Background="#FFDDDDDD" FontWeight="Bold"/>
        <Button x:Name="logout" Grid.ColumnSpan="2" Content="Log Out" HorizontalAlignment="Left" VerticalAlignment="Top" Width="198" Margin="24,608,0,0" Height="69" Click="logout_Click_1" BorderBrush="#FF888888" Background="#FFE01B1B" FontWeight="Bold" FontSize="18"/>
        <Label x:Name="Title" Grid.ColumnSpan="2" Content="Capstone Employee Scheduler" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="156,46,0,0" Height="100" Width="740" FontWeight="Bold" FontSize="48" RenderTransformOrigin="0.145,0.288"/>
        <Button x:Name="GenerateButton" Grid.ColumnSpan="2" Content="Generate Schedule" HorizontalAlignment="Left" VerticalAlignment="Top" Width="198" Margin="746,608,0,0" FontWeight="Bold" Height="69" FontSize="18" Click="GenerateButton_Click"/>

    </Grid>
</Window>
