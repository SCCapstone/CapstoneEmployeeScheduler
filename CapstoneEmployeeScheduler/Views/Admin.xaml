<Page x:Class="CapstoneEmployeeScheduler.Views.admin"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CapstoneEmployeeScheduler.Views"
      mc:Ignorable="d" 
      d:DesignHeight="349.6" d:DesignWidth="367.2"
      Title="admin">
    <Grid>
        <CheckBox x:Name="employee" Content="Employee Page" HorizontalAlignment="Left" Margin="130,80,0,0" VerticalAlignment="Top" />
        <CheckBox x:Name="roles" Content="Roles Page" HorizontalAlignment="Left" Margin="130,105,0,0" VerticalAlignment="Top" />
        <CheckBox x:Name="history" Content="View Previous Schedules" HorizontalAlignment="Left" Margin="130,130,0,0" VerticalAlignment="Top" />
        <CheckBox x:Name="view" Content="View Today's Schedule" HorizontalAlignment="Left" Margin="130,155,0,0" VerticalAlignment="Top" />
        <CheckBox x:Name="generate" Content="Generate Schedule" HorizontalAlignment="Left" Margin="130,180,0,0" VerticalAlignment="Top" />
        <Button x:Name="submit" Content="Submit" HorizontalAlignment="Left" Margin="150,245,0,0" VerticalAlignment="Top" Width="75" Click="Submit_Click" />
    </Grid>



</Page>
